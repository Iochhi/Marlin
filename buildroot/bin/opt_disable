    for name in args:
        changed = False

        for file in config.FILES:
            if os.path.exists(file):
                if config.enable(file, name, False):
                    changed = True

        if not changed:
            print(f"ERROR: Can't find {name}")
            exit(1)

if __name__ == "__main__":
    main()
